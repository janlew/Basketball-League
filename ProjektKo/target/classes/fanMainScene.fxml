<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" stylesheets="styles/fanMain.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.league.controllers.FanMainSceneController">
   <children>
      <Button fx:id="stand" layoutX="30.0" layoutY="301.0" mnemonicParsing="false" onAction="#checkStandings" prefWidth="150" text="Check standings" />
      <Button fx:id="res" layoutX="230.0" layoutY="301.0" mnemonicParsing="false" onAction="#checkResults" prefWidth="150" text="Results" />
      <Button fx:id="sign" layoutX="533.0" layoutY="2.0" mnemonicParsing="false" onAction="#signIn" text="Sign in" />
      <Button fx:id="player" layoutX="430.0" layoutY="301.0" mnemonicParsing="false" onAction="#checkPlayers" prefWidth="150" text="Players" />
      <ImageView fitHeight="250.0" fitWidth="350.0" layoutX="125.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true">
              <image>
                 <Image url="images/logo.png" />
              </image>
      </ImageView>
   </children>
</AnchorPane>
